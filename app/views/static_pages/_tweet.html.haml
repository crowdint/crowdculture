      %a.comment{href:entry.content_url, target: '_blank'}
      %a.retweet{"data-lang" => "en", "data-text" => entry.title, href: '#'}
      %a.favorite{href:entry.content_url, target: '_blank'}
    %h1
      %a{href: entry.content_url, target:'_blank'}
        =raw "#{entry.title}"